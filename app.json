{
  "expo": {
    "name": "Framez",
    "slug": "Framez",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "framez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "convexUrl": "https://scintillating-puffin-719.convex.cloud",
      "router": {},
      "eas": {
        "projectId": "6664601a-52c0-4c0b-b271-d62f49516bd4"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.framez.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
